<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.athome.dao.BookDao">

  <select id="get" resultType="com.athome.entity.Book">
    select id,total,name from book where id =#{id}
  </select>

  <select id="list" resultType="com.athome.entity.Book">
    select id,total,name from book
  </select>
</mapper>